<html>
	<body>
		<table border="1">
			<tr>
				<td>ID</td>
				<td>TÍTULO</td>
				<td>DESCRIÇÃO</td>
				<td>PREÇO</td>
			</tr>
		
			<% for(var i = 0; i < lista.length; i++){%>
				<tr>
					<td><%= lista[i].id %></td>
					<td><%= lista[i].titulo %></td>
					<td><%= lista[i].descricao %></td>
					<td><%= lista[i].preco %></td>
				</tr>
			<% } %>
		</table>
	</body>
</html>